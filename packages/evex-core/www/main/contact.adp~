<script>
  function joinow(contact){
    if(id=="contact"){
	$("div.on1").fadeIn("slow");
	$(".overOuter_joinow").fadeIn("slow");
	$('.overOuter_joinow').addClass('displayBlock');
	$("#joinow_outer").fadeIn("slow");	
	$("#pop2").addClass("displayBlock");	
	$(".headerwrapper").css('z-index','9');
	$('body,html').animate({
	  scrollTop: 0
	}, 800);
    }
  }

  function close_joinnow(){
	 $("div.on1").fadeOut("slow");
	 $(".overOuter_joinow").fadeOut("slow");
	 $('.overOuter_joinow').removeClass('displayBlock');
	 $("#joinow_outer").removeClass('displayBlock');
	 $("#joinow_outer").hide();	
	 $("#pop2").removeClass("displayBlock");
		$(".headerwrapper").css("z-index","999");	
  }


</script>



<div id="on1">