    <script>

      function initMap() {
      var map = new google.maps.Map(document.getElementById('map'), {
      zoom: 11,
      center: {lat: 41.876, lng: -87.624}
      });

      var ctaLayer = new google.maps.KmlLayer({
      url: 'http://1c.1contact.ch/file/2573/GE.kml',
      map: map
      });
      }

    </script>
    <script async defer
	    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDzF7IlGi2Ue-EI6E6bizGVZ69NhFU7yGI&signed_in=false&callback=initMap">
    </script>
