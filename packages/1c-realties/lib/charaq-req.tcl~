ad_page_contract {

}

set package_id 2393
set tree_id 1067

multirow create characs id name

foreach {id name} [1c_annonce::get_categories -package_id $package_id -tree_id $tree_id] {
    multirow create options value

    
    foreach elem [1c_annonce::category_get_options -parent_id $id] {
	set value [lindex $elem 0]

	multirow append options $value
	
    }

    
    
    multirow append characs $id $name

}

